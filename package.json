{
    "name": "skillcaptain-mcp-server",
    "version": "1.1.0",
    "description": "MCP server for interacting with SkillCaptain API - manage courses, assignments, and submit code for evaluation",
    "main": "build/index.js",
    "type": "module",
    "bin": {
      "skillcaptain-mcp": "build/index.js"
    },
    "files": [
      "build"
    ],
    "scripts": {
      "build": "tsc",
      "prepare": "npm run build",
      "watch": "tsc --watch",
      "dev": "npm run build && node build/index.js"
    },
    "keywords": [
      "mcp",
      "model-context-protocol",
      "skillcaptain",
      "claude",
      "ai",
      "education",
      "coding",
      "learning"
    ],
    "author": "hello@skillcaptain.app",
    "license": "MIT",
    "repository": {
      "type": "git",
      "url": "https://github.com/skillCaptain/skillcaptain-mcp.git"
    },
    "bugs": {
      "url": "https://github.com/skillCaptain/skillcaptain-mcp/issues"
    },
    "homepage": "https://github.com/skillCaptain/skillcaptain-mcp#readme",
    "dependencies": {
      "@modelcontextprotocol/sdk": "^0.5.0",
      "node-fetch": "^3.3.2"
    },
    "devDependencies": {
      "@types/node": "^20.11.0",
      "typescript": "^5.3.3"
    },
    "engines": {
      "node": ">=18.0.0"
    }
  }